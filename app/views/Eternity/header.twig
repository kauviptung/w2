			#block_342 .navbar-collapse {
				padding-top: 0;
			}
			#block_342 .navbar-languages {
				margin-left: 24px;
				display: flex;
				align-items: center;
			}


			.body .navbar-dropdown {
				position: relative;
				display: inline-block;
				margin-top: 4px;
				padding-bottom: 4px;
				font-size: "";
			}

			.body .navbar-dropdown__name {
				white-space: nowrap;
				display: flex;
				justify-content: space-between;
				align-items: center;
				color: #07264a;
				background: #ffffff;
				border-color: transparent;
				box-shadow: 0 8px 24px 0 rgba(0, 0, 0, 0.2);
				border-style: solid;
				border-left-width: 1px;
				border-right-width: 1px;
				border-top-width: 1px;
				border-bottom-width: 1px;
				padding: 6px 12px;
				border-top-left-radius: 12px;
				border-top-right-radius: 12px;
				border-bottom-left-radius: 12px;
				border-bottom-right-radius: 12px;
				font-size: "";
				font-weight: 600;
				font-style: normal;
			}

			.body .navbar-dropdown__container {
				position: absolute;
				padding: 0;
				margin: 5px 0 0;
				top: 100%;
				background: #ffffff;
				min-width: 100%;
				border-top-left-radius: 4px;
				border-top-right-radius: 4px;
				border-bottom-left-radius: 4px;
				border-bottom-right-radius: 4px;
				border-color: transparent;
				box-shadow: 0 8px 24px 0 rgba(0, 0, 0, 0.2);
				border-style: solid;
				border-left-width: 0;
				border-right-width: 0;
				border-top-width: 0;
				border-bottom-width: 0;
			}

			.body .navbar-dropdown__link {
				display: block;
				width: 100%;
				text-decoration: none;
				font-size: 1em;
				color: #07264a;
				padding: 6px 12px;
			}

			.body .navbar-dropdown__item {
				list-style: none;
			}

			.body .navbar-dropdown__toggle:after {
				display: inline-block;
				content: '\f107';
				font-family: 'Font Awesome 5 Pro';
				margin-left: 12px;
			}

			.body-rtl {
				direction: rtl;
				text-align: inherit !important;
			}

			@media(max-width: 991px) {
				#block_342 .navbar-languages {
					margin-top: 20px;
					margin-left: 0;
				}
				.body .navbar-dropdown {
					width: 100%;
					margin-top: 0;
					padding-bottom: 0;
				}
				.body .navbar-dropdown__name {
					width: 100%;
				}
				.body .navbar-dropdown__container {
					position: relative;
					visibility: visible;
				}

			}


			{% if settings["site_theme_alt"] == "lilac" %}


				#service_description,
				#service_avg_time {
					padding: 13px;
					border-radius: 10px;
					color: #393e46;
					box-shadow: 1px 1px 1px #F8F0FF;
					background-color: #F8F0FF;
				}

